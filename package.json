{
  "name": "gradexis",
  "private": true,
  "version": "1.0.0",
  "description": "Gradexis",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "api": "cd gradexis-api && nodemon index.js",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "all": "concurrently \"npm run dev\" \"npm run api\"",
    "build": "cross-env NODE_ENV=production vite build && npx cap sync",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "^6.0.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.2.0",
    "@capacitor/ios": "^6.2.0",
    "@capacitor/status-bar": "^6.0.1",
    "@hugotomazi/capacitor-navigation-bar": "^4.0.1",
    "@material/material-color-utilities": "^0.3.0",
    "axios": "^1.7.7",
    "axios-cookiejar-support": "^5.0.5",
    "cheerio": "^1.0.0",
    "color-convert": "^2.0.1",
    "cors": "^2.8.5",
    "dom7": "^4.0.6",
    "express": "^4.21.2",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.4",
    "material-icons": "^1.13.12",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "skeleton-elements": "^4.0.1",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss-preset-env": "^10.0.3",
    "vite": "^5.4.6",
    "vite-plugin-terminal": "^1.2.0"
  }
}
